<h1>Editing guild</h1>

<% form_for(@guild) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :officer_rank %><br />
    <%= f.text_field :officer_rank %>
  </p>
  <p>
    <%= f.label :raidleader_rank %><br />
    <%= f.text_field :raidleader_rank %>
  </p>
  <p>
    <%= f.label :website %><br />
    <%= f.text_field :website %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @guild %> |
<%= link_to 'Back', guilds_path %>